{
  "name":"heart-bits/contao-contacts-bundle",
  "description":"Contacts extension for Contao Open Source CMS",
  "keywords":["contao", "contacts", "kontakte"],
  "type":"contao-bundle",
  "license":"LGPL-3.0+",
  "authors":[
    {
      "name":"heart-bits",
      "homepage":"http://heart-bits.com",
      "role":"Developer"
    }
  ],
  "support":{
  	"email": "hi@heart-bits.com",
		"issues":"https://github.com/heart-bits/hb-contacts-bundle/issues",
		"source":"https://github.com/heart-bits/hb-contacts-bundle"
	},
  "require":{
    "php":">=7.1.0",
    "contao/core-bundle": "~4.9",
    "delahaye/dlh_geocode": "~1.0"
  },
  "require-dev": {
    "contao/manager-bundle": "~4.9"
  },
  "autoload": {
    "psr-4": { "Heartbits\\ContaoContacts\\": "src/" },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra":{
    "contao-manager-plugin": "Heartbits\\ContaoContacts\\ContaoManager\\Plugin"
  }
}
